#################################################################
#    Rules
#################################################################

<urn:swrl:var#eqc> rdf:type swrl:Variable .

<urn:swrl:var#d> rdf:type swrl:Variable .

<urn:swrl:var#eq> rdf:type swrl:Variable .

<urn:swrl:var#mp> rdf:type swrl:Variable .

<urn:swrl:var#p> rdf:type swrl:Variable .

<urn:swrl:var#c> rdf:type swrl:Variable .

[ rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate <https://w3id.org/tribont/core#hasDocument> ;
                           swrl:argument1 <urn:swrl:var#eqc> ;
                           swrl:argument2 <urn:swrl:var#d>

    </owl:Class>
    


    <!-- https://w3id.org/tribont/sample#LubricationSample -->

    <owl:Class rdf:about="https://w3id.org/tribont/sample#LubricationSample">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/tribont/sample#Sample"/>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://w3id.org/tribont/sample#hasCoreMaterial"/>
                                <owl:someValuesFrom rdf:resource="https://w3id.org/tribont/material#FluidLubricant"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://w3id.org/tribont/sample#hasCoreMaterial"/>
                                <owl:someValuesFrom rdf:resource="https://w3id.org/tribont/material#SolidLubricant"/>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://w3id.org/tribont/sample#Sample"/>
        <rdfs:comment>Portion of lubricant material selected from a larger quantity of lubricant material for testing purposes. Lubricant material can be fluid or solid lubricants </rdfs:comment>
        <rdfs:isDefinedBy>https://w3id.org/tribont/sample</rdfs:isDefinedBy>
        <rdfs:label>LubricationSample</rdfs:label>
        <ns1:term_status>Testing</ns1:term_status>
    </owl:Class>
    



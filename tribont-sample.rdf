    <!-- https://w3id.org/tribont/sample#LubricatedSampleSystem -->

    <owl:Class rdf:about="https://w3id.org/tribont/sample#LubricatedSampleSystem">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/tribont/sample#SampleSystem"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/tribont/sample#hasLubricationSample"/>
                        <owl:someValuesFrom rdf:resource="https://w3id.org/tribont/sample#LubricationSample"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://w3id.org/tribont/sample#SampleSystem"/>
        <rdfs:comment>Sample system that includes a lubricant as part of it.</rdfs:comment>
        <rdfs:isDefinedBy>https://w3id.org/tribont/sample</rdfs:isDefinedBy>
        <rdfs:label>LubricatedSampleSystem</rdfs:label>

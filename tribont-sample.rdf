    <!-- https://w3id.org/tribont/sample#CoatedSample -->

    <owl:Class rdf:about="https://w3id.org/tribont/sample#CoatedSample">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/tribont/sample#Sample"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/tribont/sample#hasCoating"/>
                        <owl:someValuesFrom rdf:resource="https://w3id.org/tribont/sample#Coating"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://w3id.org/tribont/sample#Sample"/>
        <rdfs:comment>Sample body which has a coating moterial.</rdfs:comment>
        <rdfs:isDefinedBy>https://w3id.org/tribont/sample</rdfs:isDefinedBy>
        <rdfs:label>CoatedSample</rdfs:label>
        <ns1:term_status>Testing</ns1:term_status>
    </owl:Class>
    


    <!-- https://w3id.org/tribont/sample#Coating -->

    <owl:Class rdf:about="https://w3id.org/tribont/sample#Coating">
        <rdfs:subClassOf rdf:resource="https://w3id.org/isCharacterisedBy#SubjectOfInterest"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isCharacterisedBy#isCharacterisedBy"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/tribont/sample#CoatingProperty"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/tribont/sample#hasCoreMaterial"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/tribont/material#Material"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/tribont/sample#hasCoreMaterial"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/tribont/material#Material"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://w3id.org/tribont/sample#CoatingProperty -->


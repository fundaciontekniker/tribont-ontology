                            rdfs:comment "An aggrupation of experimental equipments with a set of common atributes of characteristics." ;
                            rdfs:isDefinedBy "https://w3id.org/tribont/equipment" ;
                            rdfs:label "ExperimentalEquipmentClass" ;

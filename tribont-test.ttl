###  https://w3id.org/tribont/test#Experiment
test:Experiment rdf:type owl:Class ;
                rdfs:subClassOf <http://org.semanticweb.owlapi/error#Error72> ,
                                <http://org.semanticweb.owlapi/error#Error73> ,
                                <https://w3id.org/isCharacterisedBy#SubjectOfInterest> ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <https://w3id.org/tribont/core#hasDocument> ;
                                  owl:someValuesFrom test:TestOutputDocument
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <https://w3id.org/tribont/core#hasDocument> ;
                                  owl:allValuesFrom test:TestOutputDocument
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <https://w3id.org/tribont/core#isMadeOf> ;
                                  owl:allValuesFrom test:Test
                                ] ;
                rdfs:comment "A series of activities to carry out an experimentation procedure and measure its outputs." ;
                rdfs:isDefinedBy "https://w3id.org/tribont/test" ;
                rdfs:label "Experiment" ;
                ns1:term_status "Testing" .


###  https://w3id.org/tribont/test#ExperimentalTest
test:ExperimentalTest rdf:type owl:Class ;
                      rdfs:subClassOf test:Test ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty test:isExecutedBy ;
                                        owl:allValuesFrom <https://w3id.org/tribont/equipment#ExperimentalEquipment>
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty test:isExecutedBy ;
                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onClass <https://w3id.org/tribont/equipment#ExperimentalEquipment>
                                      ] ;
                      rdfs:comment "A testing activity that reproduces certain operation conditions to assess one or more technical attributes or characteristics of a given sample or sample system." ;
                      rdfs:isDefinedBy "https://w3id.org/tribont/test" ;
                      rdfs:label "ExperimentalTest" ;
                      ns1:term_status "Testing" .


###  https://w3id.org/tribont/test#MeasuringTest
test:MeasuringTest rdf:type owl:Class ;
                   rdfs:subClassOf test:Test ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty test:isExecutedBy ;
                                     owl:allValuesFrom <https://w3id.org/tribont/equipment#MeasuringEquipment>
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty test:testsSample ;
                                     owl:allValuesFrom <https://w3id.org/tribont/sample#Sample>
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty test:isExecutedBy ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass <https://w3id.org/tribont/equipment#MeasuringEquipment>
                                   ] ;
                   rdfs:comment "A testing activity that measures one or more attributes or characteristics of a given sample." ;
                   rdfs:isDefinedBy "https://w3id.org/tribont/test" ;
                   rdfs:label "MeasuringTest" ;
                   ns1:term_status "Testing" .


###  https://w3id.org/tribont/test#OperationConfiguration
test:OperationConfiguration rdf:type owl:Class ;
                            rdfs:subClassOf <https://w3id.org/tribont/core#OperationCondition> ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty <https://w3id.org/tribont/core#hasRole> ;
                                              owl:hasValue <https://w3id.org/tribont/core#configuration>
                                            ] ;
                            rdfs:comment "Logical aggregation of nominal value of operation attributes, or characteristics of something." ;
                            rdfs:isDefinedBy "https://w3id.org/tribont/test" ;
                            rdfs:label "OperationConfiguration" ;
                            ns1:term_status "Testing" .


###  https://w3id.org/tribont/test#OperationTerminationMeasurement
test:OperationTerminationMeasurement rdf:type owl:Class ;
                                     rdfs:subClassOf <https://w3id.org/tribont/core#OperationCondition> ,
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty <https://w3id.org/tribont/core#hasRole> ;
                                                       owl:hasValue <https://w3id.org/tribont/core#measurement>
                                                     ] ;
                                     rdfs:comment "Logical aggregation of the values of the operation attributes, or characteristics measured when a test is terminated." ;
                                     rdfs:isDefinedBy "https://w3id.org/tribont/test" ;
                                     rdfs:label "OperationTerminationMeasurement" ;
                                     ns1:term_status "Testing" .


###  https://w3id.org/tribont/test#Operator
test:Operator rdf:type owl:Class ;
              rdfs:subClassOf <http://org.semanticweb.owlapi/error#Error74> ,
                              <http://org.semanticweb.owlapi/error#Error75> ,
                              <https://purl.industrialontologies.org/ontology/core/Core/Person> ;
              rdfs:comment "Person performing an specific activity or operation." ;
              rdfs:isDefinedBy "https://w3id.org/tribont/test" ;
              rdfs:label "Operator" ;
              ns1:term_status "Testing" .


###  https://w3id.org/tribont/test#OutputMeasurement
test:OutputMeasurement rdf:type owl:Class ;
                       rdfs:subClassOf <https://w3id.org/tribont/core#Output> ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty <https://w3id.org/tribont/core#hasRole> ;
                                         owl:hasValue <https://w3id.org/tribont/core#measurement>
                                       ] ;
                       rdfs:comment "Logical aggregation of the measured values of the technical attributes, or characteristics obtained as output of a test." ;
                       rdfs:isDefinedBy "https://w3id.org/tribont/test" ;
                       rdfs:label "OutputMeasurement" ;
                       ns1:term_status "Testing" .


###  https://w3id.org/tribont/test#Test
test:Test rdf:type owl:Class ;
          rdfs:subClassOf <http://org.semanticweb.owlapi/error#Error76> ,
                          <http://org.semanticweb.owlapi/error#Error77> ,
                          <https://w3id.org/isCharacterisedBy#SubjectOfInterest> ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <https://w3id.org/tribont/core#hasDocument> ;
                            owl:someValuesFrom test:TestOutputDocument
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <https://w3id.org/isCharacterisedBy#isCharacterisedBy> ;
                            owl:allValuesFrom [ rdf:type owl:Class ;
                                                owl:unionOf ( <https://w3id.org/tribont/core#GeometricDimension>
                                                              <https://w3id.org/tribont/core#OperationProperty>
                                                              <https://w3id.org/tribont/core#TechnicalProperty>
                                                              <https://w3id.org/tribont/core#Volume>
                                                              <https://w3id.org/tribont/core#Weight>
                                                              test:OperationConfiguration
                                                              test:OperationTerminationMeasurement
                                                              test:OutputMeasurement
                                                            )
                                              ]
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <https://w3id.org/tribont/core#hasDocument> ;
                            owl:allValuesFrom test:TestOutputDocument
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <https://w3id.org/tribont/core#isMadeOf> ;
                            owl:allValuesFrom test:TestStep
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty test:hasDependency ;
                            owl:allValuesFrom test:Test
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty test:isExecutedBy ;
                            owl:allValuesFrom <https://w3id.org/tribont/equipment#Equipment>
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty test:isPerformedAt ;
                            owl:allValuesFrom <http://dbpedia.org/ontology/Company>
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty test:isPerformedBy ;
                            owl:allValuesFrom test:Operator
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty test:isExecutedBy ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass <https://w3id.org/tribont/equipment#Equipment>
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty test:isPerformedAt ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass <http://dbpedia.org/ontology/Company>
                          ] ;
          rdfs:comment "An activity intended to establish the techncial attributes or characteristics of something," ;
          rdfs:isDefinedBy "https://w3id.org/tribont/test" ;
          rdfs:label "Test" ;
          ns1:term_status "Testing" .


###  https://w3id.org/tribont/test#TestOutputDocument
test:TestOutputDocument rdf:type owl:Class ;
                        rdfs:subClassOf <http://dbpedia.org/ontology/Document> ;
                        rdfs:comment "A document compiling the outputs of a given test." ;
                        rdfs:isDefinedBy "https://w3id.org/tribont/test" ;
                        rdfs:label "TestOutputDocument" ;

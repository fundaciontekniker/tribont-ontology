###  https://w3id.org/tribont/test#executes
test:executes rdf:type owl:ObjectProperty ;
              owl:inverseOf test:isExecutedBy ;
              rdfs:comment "Relation between an equipment and the activity it carries out." ;
              rdfs:isDefinedBy "https://w3id.org/tribont/test" ;
              rdfs:label "executes" ;
              ns1:term_status "Testing" .


###  https://w3id.org/tribont/test#hasAfterEndDependency
test:hasAfterEndDependency rdf:type owl:ObjectProperty ;
                           owl:equivalentProperty test:isNext ;
                           rdfs:subPropertyOf test:hasDependency ;
                           rdfs:comment "Relation between an activity and the one preceding it." ;
                           rdfs:isDefinedBy "https://w3id.org/tribont/test" ;
                           rdfs:label "hasAfterEndDependency" ;
                           ns1:term_status "Testing" .


###  https://w3id.org/tribont/test#hasDependency
test:hasDependency rdf:type owl:ObjectProperty ;
                   rdfs:comment "Identifies the dependency between two activities." ;
                   rdfs:isDefinedBy "https://w3id.org/tribont/test" ;
                   rdfs:label "hasDependency" ;
                   ns1:term_status "Testing" .


###  https://w3id.org/tribont/test#hasNofollowDependency
test:hasNofollowDependency rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf test:hasDependency ;
                           rdfs:comment "Indicates that an activity has not a following one." ;
                           rdfs:isDefinedBy "https://w3id.org/tribont/test" ;
                           rdfs:label "hasNofollowDependency" ;
                           ns1:term_status "Testing" .


###  https://w3id.org/tribont/test#isExecutedBy
test:isExecutedBy rdf:type owl:ObjectProperty ;
                  rdfs:comment "Relation between an activity and the equipment that executes it." ;
                  rdfs:isDefinedBy "https://w3id.org/tribont/test" ;
                  rdfs:label "isExecutedBy" ;
                  ns1:term_status "Testing" .


###  https://w3id.org/tribont/test#isNext
test:isNext rdf:type owl:ObjectProperty ;
            owl:inverseOf test:isPrevious ;
            rdfs:comment "Relation between an activity and the one preceding it." ;
            rdfs:isDefinedBy "https://w3id.org/tribont/test" ;
            rdfs:label "isNext" ;
            ns1:term_status "Testing" .


###  https://w3id.org/tribont/test#isPerformedAt
test:isPerformedAt rdf:type owl:ObjectProperty ;
                   rdfs:comment "Relation between an activity and the company where it is performed." ;
                   rdfs:isDefinedBy "https://w3id.org/tribont/test" ;
                   rdfs:label "isPerformedAt" ;
                   ns1:term_status "Testing" .


###  https://w3id.org/tribont/test#isPerformedBy
test:isPerformedBy rdf:type owl:ObjectProperty ;
                   owl:inverseOf test:performs ;
                   rdfs:comment "Relation between an activity and the person who performs it." ;
                   rdfs:isDefinedBy "https://w3id.org/tribont/test" ;
                   rdfs:label "isPerformedBy" ;
                   ns1:term_status "Testing" .


###  https://w3id.org/tribont/test#isPrevious
test:isPrevious rdf:type owl:ObjectProperty ;
                rdfs:comment "Relation between an activity and the one following it." ;
                rdfs:isDefinedBy "https://w3id.org/tribont/test" ;
                rdfs:label "isPrevious" ;
                ns1:term_status "Testing" .


###  https://w3id.org/tribont/test#performs
test:performs rdf:type owl:ObjectProperty ;
              rdfs:comment "The relation between a person and the activity he/she performs " ;
              rdfs:isDefinedBy "https://w3id.org/tribont/test" ;
              rdfs:label "performs" ;
              ns1:term_status "Testing" .


###  https://w3id.org/tribont/test#testsSample
test:testsSample rdf:type owl:ObjectProperty ;
                 rdfs:comment "Relation between a testing activity and the involved sample." ;
                 rdfs:isDefinedBy "https://w3id.org/tribont/test" ;
                 rdfs:label "testsSample" ;
                 ns1:term_status "Testing" .


###  https://w3id.org/tribont/test#testsSampleSystem
test:testsSampleSystem rdf:type owl:ObjectProperty ;
                       rdfs:comment "Relation between a testing activity and the involved sample system." ;
                       rdfs:isDefinedBy "https://w3id.org/tribont/test" ;
                       rdfs:label "testsSampleSystem" ;
                       ns1:term_status "Testing" .


